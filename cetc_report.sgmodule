#!name=电科健康填报
#!desc=每日自动填报电科平台的健康数据
#!system=ios

[Script]
抗疫_电科自动填报 = type=cron,cronexp=0 0 * * *,timeout=120,wake-system=1,script-path=https://raw.githubusercontent.com/codetracer/chons/master/cetc_report.js
抗疫_电科登录获取Token = type=http-request,requires-body=true,max-size=0,pattern=^https:\/\/asst\.cetccloud\.com\/ncov\/login$,script-path=https://raw.githubusercontent.com/codetracer/chons/master/cetc_report.js
抗疫_电科填报获取内容 = type=http-request,requires-body=true,max-size=0,pattern=^https:\/\/asst\.cetccloud\.com\/oort\/.*2019.*\/(edit_)?everyday_report,script-path=https://raw.githubusercontent.com/codetracer/chons/master/cetc_report.js
MagicJS_变量维护 = type=http-request,requires-body=1,max-size=0,pattern=^http:\/\/(www\.)?magic\.js\/value\/(write|read|del),script-path=https://raw.githubusercontent.com/codetracer/horus/master/script/magicjs/magic.js

[MITM]
hostname = %APPEND% asst.cetccloud.com,captcha.cetccloud.com